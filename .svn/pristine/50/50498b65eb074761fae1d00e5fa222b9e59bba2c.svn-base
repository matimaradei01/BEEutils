export class Contador{
    private ORDEN_CON_UQ : number = 1;
    private ORDEN_CON_FK : number = 1;

    sumarUQ(){
        this.ORDEN_CON_UQ++;
    }

    
    sumarFK(){
        this.ORDEN_CON_FK++;
    }

    getContadorFK(){
        return this.ORDEN_CON_FK;
    }

    getContadorUQ(){
        return this.ORDEN_CON_UQ;
    }
}